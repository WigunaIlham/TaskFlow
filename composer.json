    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/breeze": "^2.0",
        "laravel/sanctum": "^4.2",
        "inertiajs/inertia-laravel": "^2.0",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
